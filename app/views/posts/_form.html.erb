<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :url %>
  <%= f.text_field :url %><br>
  <%= f.label :description %>
  <%= f.text_field :description %><br>
  <%= f.label :category %>
  <%= f.select :category, options_for_select([['General', 'General'], ['Tech', 'Tech'],['Funny', 'Funny'],['Entertainment', 'Entertainment'], ['News', 'News']])%><br>
  <%= f.submit %>
<% end %>
